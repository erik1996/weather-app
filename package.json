{
  "name": "weather-and-forecast-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "1.8.0",
    "axios": "0.26.1",
    "clsx": "1.1.1",
    "env-cmd": "10.1.0",
    "moment": "^2.29.1",
    "normalize.css": "8.0.1",
    "query-string": "7.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-inlinesvg": "2.3.0",
    "react-redux": "7.2.6",
    "react-router-dom": "6.2.2",
    "rooks": "5.10.2",
    "uuid": "8.3.2"
  },
  "scripts": {
    "build:production": "env-cmd -f ./.env.production react-scripts build",
    "build:staging": "env-cmd -f ./.env.staging react-scripts build",
    "start": "env-cmd -f ./.env.development react-scripts start",
    "start:ci": "env-cmd -f ./.env.testing react-scripts start",
    "storybook": "start-storybook -p 6006 -s public",
    "test": "yarn test:unit && yarn test:e2e",
    "test:e2e": "start-server-and-test 'yarn start:ci' http://localhost:3000 'yarn cypress run --browser chrome'",
    "test:e2e:open": "start-server-and-test 'yarn start:ci' http://localhost:3000 'yarn cypress open'",
    "test:unit": "react-scripts test"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@mvp-tech/lint-presets": "1.1.0",
    "@storybook/addon-actions": "6.4.19",
    "@storybook/addon-essentials": "6.4.19",
    "@storybook/addon-knobs": "6.4.0",
    "@storybook/addon-links": "6.4.19",
    "@storybook/builder-webpack5": "6.4.19",
    "@storybook/manager-webpack5": "6.4.19",
    "@storybook/node-logger": "6.4.19",
    "@storybook/preset-create-react-app": "4.1.0",
    "@storybook/react": "6.4.19",
    "@testing-library/cypress": "8.0.2",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.4",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.4.1",
    "@types/node": "16.7.13",
    "@types/react": "17.0.40",
    "@types/react-dom": "17.0.13",
    "@types/redux-mock-store": "1.0.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "5.15.0",
    "@typescript-eslint/parser": "5.15.0",
    "cypress": "9.5.2",
    "eslint": "8.11.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "16.1.3",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-dirnames": "1.0.3",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-unicorn": "41.0.1",
    "node-sass": "7.0.1",
    "prettier": "2.6.0",
    "react-scripts": "5.0.0",
    "redux-mock-store": "1.5.4",
    "start-server-and-test": "1.14.0",
    "stylelint": "14.6.0",
    "stylelint-config-sass-guidelines": "9.0.1",
    "typescript": "4.6.2",
    "webpack": "5.70.0"
  }
}
